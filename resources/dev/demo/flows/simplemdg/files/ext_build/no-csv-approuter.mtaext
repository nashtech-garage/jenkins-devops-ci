_schema-version: '3.1'
ID: simple-mdg-updatemode
extends: simple-mdg

modules:
  - name: simple-mdg-web
    build-parameters:
      no-source: true

  - name: simple-mdg-srv
    parameters:
      keep-existing:
        env: true

  - name: simple-mdg-srv-bg
    parameters:
      keep-existing:
        env: true
  
  - name: simple-mdg-db
    build-parameters:
      ignore: ["package-lock.json", "node_modules/", "src/csv"]