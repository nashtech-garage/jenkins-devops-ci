_schema-version: '3.1'
ID: simple-mdg-fresh
extends: simple-mdg

modules:
  - name: simple-mdg-db
    parameters:
      tasks:
        - name: task-1
          command: node node_modules/@sap/hdi-deploy/
        - name: task-2
          command: "node node_modules/@sap/hdi-deploy/ --undeploy ./db/src/csv/*.hdbtabledata --path-parameter ./db/src/csv/*.hdbtabledata:skip_data_deletion=true"